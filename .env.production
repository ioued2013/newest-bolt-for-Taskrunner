# Production Environment Variables
NODE_ENV=production
APP_NAME=Task Runner
APP_SLUG=task-runner
APP_VERSION=1.0.0
IOS_BUNDLE_ID=com.taskrunner.app
ANDROID_PACKAGE=com.taskrunner.app
ANDROID_VERSION_CODE=1
IOS_BUILD_NUMBER=1.0.0

# Supabase Configuration (Replace with your production values)
EXPO_PUBLIC_SUPABASE_URL=your_production_supabase_url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_production_supabase_anon_key

# Stripe Configuration (Replace with your production values)
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_production_stripe_publishable_key

# EAS Configuration
EAS_PROJECT_ID=your_eas_project_id

# Analytics and Monitoring
EXPO_PUBLIC_ANALYTICS_ENABLED=true
EXPO_PUBLIC_ERROR_REPORTING_ENABLED=true

# Feature Flags
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true
EXPO_PUBLIC_ENABLE_REAL_TIME_FEATURES=true
EXPO_PUBLIC_ENABLE_ADVANCED_ANALYTICS=false